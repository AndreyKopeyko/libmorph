// Таблицы окончаний для существительных, входящих в устойчивые словосочетания
// и имеющих не все формы.

.тип м 1a:m1, м 1b:m1, мо 1a:m1
//--------------------------------------------------------------------------
// Пример: ляд, едун
// Существительные, имеющие только именительный падеж
//--------------------------------------------------------------------------
{
  .число Е
    '', И               // ляд
}

.тип м 1a:m2
//--------------------------------------------------------------------------
// Пример: чес, блезир (дать чесу), туз (дать туза)
// Существительные, имеющие только родительный падеж
//--------------------------------------------------------------------------
{
  .число Е
    у, Р2               // блезир-у
}

.тип м 1a:m2+, м 1b:m2
//--------------------------------------------------------------------------
// Пример: бельмес
// Существительные, имеющие только родительный падеж
//--------------------------------------------------------------------------
{
  .число Е
    а, Р                // бельмес-а
}

.тип м 1a:m2m2
//--------------------------------------------------------------------------
// Пример: призор
// Существительные, имеющие только 2 родительных падежа
//--------------------------------------------------------------------------
{
  .число Е
    а, Р                // без призор-а
    у, Р2               // без призор-у
}

.тип м 1a:m25
//--------------------------------------------------------------------------
// Пример: спуд
// Существительные, имеющие только родительный и творительный падежи
//--------------------------------------------------------------------------
{
  .число Е
    а, Р                // из-под спуда
    ом, Т               // под спудом
}

.тип м 1a:m4
//--------------------------------------------------------------------------
// Пример: опор
// Существительные, имеющие только винительный падеж
//--------------------------------------------------------------------------
{
  .число Е
    '', В               // во весь опор
}

.тип м 1a:m45
//--------------------------------------------------------------------------
// Пример: манер
// Существительные, имеющие только винительный и творительный падежи
//--------------------------------------------------------------------------
{
  .число Е
    '', В               // на манер
    ом, Т               // таким манер-ом
}

.тип м 1a:m5
//--------------------------------------------------------------------------
// Пример: измор
// Существительные, имеющие только творительный падеж
//--------------------------------------------------------------------------
{
  .число Е
    ом, Т               // взять измор-ом
}

.тип м 1a:m6
//--------------------------------------------------------------------------
// Пример: вес
// Существительные, имеющие только второй предложный падеж
//--------------------------------------------------------------------------
{
  .число Е
    у, П2               // на вес-у
}

.тип м 1a:m6+
//--------------------------------------------------------------------------
// Пример: излет
// Существительные, имеющие только предложный падеж
//--------------------------------------------------------------------------
{
  .число Е
    е, П                // на излет-е
}

.тип м 1a"2":p4
//--------------------------------------------------------------------------
// Пример: крат (во сто крат)
// Существительные, имеющие только В. мн.
//--------------------------------------------------------------------------
{
  .число М
    '', В               // во сто крат
}

.тип м 1a:p5
//--------------------------------------------------------------------------
// Пример: онер
// Существительные, имеющие только творительный падеж множественного числа
//--------------------------------------------------------------------------
{
  .число М
    ами, Т              // со всеми онер-ами
}

.тип м 1a:m1426
//--------------------------------------------------------------------------
// Пример: лет (с лету)
//--------------------------------------------------------------------------
{
  .число Е
    '', И
    а, Р
    у, Р2               // блезир-у
    '', В
    е, П
}

