// Подтипы: 12a1 - глаголы на -уть
//          12a2 - глаголы на -ыть
//          12a3 - слово греть и производные
//          12a4 - слова почить и опочить
//          12a5 - слово брить и производные

.тип нсв 12a1Ф, нсв 12a3Ф
//**************************************************************************
// Пример: дуть(ся)
//**************************************************************************
{
.инфинитив
    ть,,,"1"            // ду-ть(ся)
.повел
    й,,"7"              // ду-й(ся), ду-й-те(сь)
.время Н        // настоящее время
  .лицо 1
    '',,"8"             // ду-ю(сь), ду-ем(ся)
  .лицо 2
    е,,"9"              // ду-е-шь(ся), ду-е-те(сь)
  .лицо 3
    '',,"10"            // ду-ет(ся), ду-ют(ся)
  .действ
    ющ,,"5"             // ду-ющ-ий(ся)
  .деепр
    я,,,"2"             // ду-я(сь)
.время П        // прошедшее время
    л,,"3"              // ду-л (-(ся),-а(сь),-о(сь),-и(сь))
  .действ
    вш,,"5"             // ду-вш -> (-ий(ся))
  .страд
    т,,"4"              // ду-т -> (-ый,-,-а,-о,-ы)
  .деепр
    в                   // ду-в
    вши,,,"2"           // ду-вши(сь)
}

