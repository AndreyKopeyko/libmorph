.время Б        // будущее время
  .лицо 1
    '',,"12"            // поднаду-ю, поднаду-ем
  .лицо 2
    е,,"13"             // поднаду-е-шь, поднаду-е-те
  .лицо 3
    '',,"14"            // поднаду-ет, поднаду-ют
.время П        // прошедшее время
    л,,"15"             // поднаду-л (-,-а,-о,-и)
  .действ
    вш,,"16"            // поднаду-вш -> (-ий)
  .страд
    т,,"4"              // поднаду-т -> (-ый,-,-а,-о,-ы)
  .деепр
    в                   // поднаду-в
    вши                 // поднаду-вши
}

