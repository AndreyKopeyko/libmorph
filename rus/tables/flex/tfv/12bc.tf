// Тип 12b

.тип нсв 12b
//**************************************************************************
// Пример: петь
//**************************************************************************
{
.инфинитив
    еть,,,"1"           // п-еть(ся)
.повел
    ой,,"11"            // п-ой, п-ой-те
.время Н        // настоящее время
  .лицо 1
    о,,"12"             // п-о-ю, п-о-ем
  .лицо 2
    ое,,"13"            // п-ое-шь, п-ое-те
  .лицо 3
    о,,"10"             // п-о-ет(ся), п-о-ют(ся)
  .действ
    оющ,,"5"            // п-оющ-ий(ся)
  .деепр
    оя                  // п-оя
.время П        // прошедшее время
    ел,,"3"             // п-ел (-(ся),-а(сь),-о(сь),-и(сь))
  .действ
    евш,,"5"            // п-евш -> (-ий(ся))
  .страд
    ет,,"4"             // п-ет -> (-ый,-,-а,-о,-ы)
  .деепр
    ев                  // п-ев
    евши                // п-евши
}

