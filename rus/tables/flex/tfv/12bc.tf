// Пример: слово гнить
//**************************************************************************
{
.инфинитив
    ть                  // гни-ть
.время Н        // настоящее время
  .лицо 1
    '',,"12"            // гни-ю, гни-ем
  .лицо 2
    е,,"13"             // гни-е-шь, гни-е-те
  .лицо 3
    '',,"14"            // гни-ет, гни-ют
  .действ
    ющ,,"16"            // гни-ющ-ий
.время П        // прошедшее время
    л,,"15"             // гни-л (-,-а,-о,-и)
  .действ
    вш,,"16"            // гни-вш -> (-ий)
  .деепр
    в                   // гни-в
    вши                 // гни-вши
}

