//---------------------------------------------------------------------------
{
.инфинитив
    еть                 // дотерп-еть
.повел
    и,,"11"             // дотерп-и(те)
.время Б        // будущее время
  .лицо 1
    '',,"44"            // дотерпл-ю, дотерп-им
  .лицо 2
    и,,"13"             // дотерп-и-шь, дотерп-и-те
  .лицо 3
    '',,"47"            // дотерп-ит, дотерп-ят
.время П        // прошедшее время
    ел,,"15"            // дотерп-ел(-,-а,-о,-и)
  .действ
    евш,,"16"           // дотерп-евш-ий
  .деепр
    ев                  // дотерп-ев
    евши                // дотерп-евши
}

