//
// Глаголы на -еть, перед которым стоит п, б, м или в
//

.тип нсв 5c2?
//---------------------------------------------------------------------------
// Пример: терпеть
//---------------------------------------------------------------------------
{
.инфинитив
    еть,,,"1"           // терп-еть(ся)
.повел
    и,,"11"             // терп-и(те)
.время Н        // настоящее время
  .лицо 1
    '',,"44"            // терпл-ю, терп-им
  .лицо 2
    и,,"13"             // терп-и-шь, терп-и-те
  .лицо 3
    '',,"48"            // терп-ит(ся), терп-ят(ся)
  .действ
    ящ,,"5"             // терп-ящ-ий(ся)
  .деепр
    я                   // терп-я
.время П        // прошедшее время
    ел,,"3"             // терп-ел(-(ся),-а(сь),-о(сь),-и(сь))
  .действ
    евш,,"5"            // терп-евш-ий(ся)
  .деепр
    ев                  // терп-ев
    евши                // терп-евши
}

.тип нсв 5c2БЯ
//---------------------------------------------------------------------------
// Пример: терпеться
//---------------------------------------------------------------------------
{
.инфинитив
    еть,,"1"            // терп-еть-ся
.время Н        // настоящее время
  .лицо 3
    '',,"50"            // терп-ит-ся
.время П        // прошедшее время
    ел,,"26"            // терп-ел-о-сь
}

