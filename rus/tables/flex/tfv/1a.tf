  .страд            // страдательное причастие
    ем,,"4"             // дела-ем -> ый, ая и т. д.
  .деепр
    я,,,"2"             // дела-я(сь)
.время П        // прошедшее время
    л,,"3"              // дела-л (-,-а,-о,-и)
  .действ
    вш,,"5"             // дела-вш -> (-ий)
  .страд
    н,,"6"              // дела-н -> (-ный,-,-а,-о,-ы)
