    я,,,"2"             // обеща-я(сь)
.время П        // прошедшее время
    л,,"3"              // разгнева-л (-,-а,-о,-и) (ся)
  .действ
    вш,,"5"             // разгнева-вш -> (-ий(ся))
  .страд
    н,,"6"              // разгнева-н-ный
  .деепр
    в                   // разгнева-в
    вши,,,"2"           // разгнева-вши(сь)
}

// Возвратные глаголы типа св 1a

.тип св 1aЯ, св 1aЯ$    // погневаться, наведаться
{
.инфинитив
    ть,,"1"             // погнева-ть-ся
.повел
    й,,"17"             // погневай-ся, погневай-те-сь
.время Б        // будущее время
  .лицо 1
    '',,"18"            // погнева-ю-сь, погнева-ем-ся
  .лицо 2
    е,,"19"             // погнева-ешься, погнева-етесь
  .лицо 3
    '',,"20"            // погнева-ется, погнева-ются
.время П        // прошедшее время
    л,,"22"             // погнева-л (-ся,-ась,-ось,-ись)
  .действ
    вш,,"21"            // погнева-вш -> ийся
  .деепр
    вши,,"2"            // погнева-вшись
}

.тип св_нп 1aБ, св_нп 1aБ$  // похолодать, прояснеть
{
.инфинитив
    ть                  // похолода-ть, проясне-ть
.время Б
  .лицо 3
    ет                  // похолода-ет, проясне-ет
.время П        // прошедшее время
    л,,"23"             // похолода-ло, проясне-ло
}

.тип св 1aБЯ            // вздуматься
{
.инфинитив
    ть,,"1"             // вздума-ться
.время Б
  .лицо 3
    '',,"27"            // вздума-ется
.время П        // прошедшее время
    л,,"26"             // вздума-лось
}

