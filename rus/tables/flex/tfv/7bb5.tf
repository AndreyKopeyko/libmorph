.тип св 7b/b5"9", св 7b/b5"9"$
//---------------------------------------------------------------------------
// Пример: соблюсти
//---------------------------------------------------------------------------
{
.инфинитив
    сти                 // соблю-сти
.повел
    ди,,"11"            // соблю-ди(те)
.время Б        // будущее время
  .лицо 1
    д,,"31"             // соблю-д-у, соблю-д-ем
  .лицо 2
    де,,"13"            // соблю-де-шь, соблю-де-те
  .лицо 3
    д,,"32"             // соблю-дет, соблю-дут
