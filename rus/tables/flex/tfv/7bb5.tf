// Подтип 7b/b5, глаголы с пометой -д-

.тип нсв 7b/b5
//---------------------------------------------------------------------------
// Пример: блюсти
//---------------------------------------------------------------------------
{
.инфинитив
    сти,,,"2"           // блю-сти(сь)
.повел
    ди,,"11"            // блю-ди(те)
.время Н        // настоящее время
  .лицо 1
    д,,"31"             // блю-д-у, блю-д-ем
  .лицо 2
    де,,"13"            // блю-де-шь, блю-де-те
  .лицо 3
    д,,"30"             // блю-дет(ся), блю-дут(ся)
  .действ
    дущ,,"5"            // блю-дущ-ий(ся)
  .деепр
    дя                  // блю-дя
.время П        // прошедшее время
    л,,"3"              // блю-л(-(ся),-ла(сь),-ло(сь),-ли(сь))
  .действ
    дш,,"5"             // блю-дш-ий(ся)
  .страд
    ден,,"6"            // блю-ден-ный
  .деепр
    дши                 // блю-дши
}

.тип нсв 7b/b5БЯ
//---------------------------------------------------------------------------
// Пример: вестись
//---------------------------------------------------------------------------
{
.инфинитив
    сти,,"2"            // ве-сти-сь
.время Н        // настоящее время
  .лицо 3
    д,,"27"             // ве-д-ет-ся
.время П        // прошедшее время
    л,,"26"             // ве-ло-сь
}

