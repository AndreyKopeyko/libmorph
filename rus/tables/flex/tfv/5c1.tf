//
// Глаголы на -еть, перед которым стоит л, н или р
//

.тип нсв 5c1Ф
//---------------------------------------------------------------------------
// Пример: смотреть
//---------------------------------------------------------------------------
{
.инфинитив
    еть,,,"1"           // смотр-еть(ся)
.повел
    и,,"28"             // смотр-и(те)(сь)
.время Н        // настоящее время
  .лицо 1
    '',,"45"            // смотр-ю(сь), смотр-им(ся)
  .лицо 2
    и,,"9"              // смотр-и-шь(ся), смотр-и-те(сь)
  .лицо 3
    '',,"48"            // смотр-ит(ся), смотр-ят(ся)
  .действ
    ящ,,"5"             // смотр-ящ-ий(ся)
  .деепр
    я,,,"2"             // смотр-я(сь)
.время П        // прошедшее время
    ел,,"3"             // смотр-ел(-(ся),-а(сь),-о(сь),-и(сь))
  .действ
    евш,,"5"            // смотр-евш-ий(ся)
  .страд
    ен,,"6"             // смотр-ен-ный
  .деепр
    ев                  // смотр-ев
    евши,,,"2"          // смотр-евши(сь)
}

