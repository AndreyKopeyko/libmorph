    '',,"47"            // обсмотр-ит, обсмотр-ят
.время П        // прошедшее время
    ел,,"15"            // обсмотр-ел(-,-а,-о,-и)
  .действ
    евш,,"16"           // обсмотр-евш-ий
  .страд
    ен,,"6"             // обсмотр-ен-ный
  .деепр
    ев                  // обсмотр-ев
    евши                // обсмотр-евши
}

