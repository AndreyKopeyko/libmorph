//
// Глаголы на -ать
//

.тип нсв 5cФ, нсв 5c?Ф
//---------------------------------------------------------------------------
// Пример: содержать
//---------------------------------------------------------------------------
{
.инфинитив
    ать,,,"1"           // содерж-ать(ся)
.повел
    и,,"28"             // содерж-и(те)(сь)
.время Н        // настоящее время
  .лицо 1
    '',,"54"            // содерж-у(сь), содерж-им(ся)
  .лицо 2
    и,,"9"              // содерж-и-шь(ся), содерж-и-те(сь)
  .лицо 3
    '',,"53"            // содерж-ит(ся), содерж-ат(ся)
  .действ
    ащ,,"5"             // содерж-ащ-ий(ся)
  .деепр
    а,,,"2"             // содерж-а(сь)
.время П        // прошедшее время
    ал,,"3"             // содерж-ал(-(ся),-а(сь),-о(сь),-и(сь))
  .действ
    авш,,"5"            // содерж-авш-ий(ся)
  .деепр
    ав                  // содерж-ав
    авши,,,"2"          // содерж-авши(сь)
}

.тип нсв 5cБЯ
//---------------------------------------------------------------------------
// Пример: дышаться
//---------------------------------------------------------------------------
{
.инфинитив
    ать,,"1"            // дыш-ать-ся
.время Н        // настоящее время
  .лицо 3
    '',,"50"            // дыш-ит-ся
.время П        // прошедшее время
    ал,,"26"            // дыш-ал-о-сь
}

.тип нсв_нп 5c"4"
