.время Н        // настоящее время
  .лицо 1
    '',,"52"            // дыш-у, дыш-им
  .лицо 2
    и,,"13"             // дыш-и-шь, дыш-и-те
  .лицо 3
    '',,"57"            // дыш-ит, дыш-ат
  .действ
    ащ,, "16"           // дыш-ащ-ий
  .деепр
    а                   // дыш-а
.время П        // прошедшее время
    ал,,"15"            // дыш-ал(-,-а,-о,-и)
  .действ
    авш,,"16"           // дыш-авш-ий
  .деепр
    ав                  // дыш-ав
    авши                // дыш-авши
}

