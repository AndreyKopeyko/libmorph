//
// Глаголы типа 8b/b, подтип 8b/b2, глаголы с пометой -к-
//

.тип нсв 8b/b2Ф
//---------------------------------------------------------------------------
// Пример: волочь
//---------------------------------------------------------------------------
{
.инфинитив
    чь,,,"1"            // воло-чь(ся)
.повел
    ки,,"28"            // воло-ки(те)(сь)
.время Н        // настоящее время
  .лицо 1
    '',,"89"            // воло-ку(сь), воло-чем(ся)
  .лицо 2
    че,,"9"             // воло-че-шь(ся), воло-че-те(сь)
  .лицо 3
    '',,"90"            // воло-чет(ся), воло-кут(ся)
  .действ
    кущ,,"5"            // воло-кущ-ий(ся)
.время П        // прошедшее время
    к,,"38"             // воло-к(-(ся),-ла(сь),-ло(сь),-ли(сь))
  .действ
    кш,,"5"             // воло-кш-ий(ся)
  .страд
    чен,,"6"            // воло-чен-ный
  .деепр
    кши,,,"2"           // воло-кши(сь)
}

.тип нсв_нп 8b/b2
//---------------------------------------------------------------------------
// Пример: течь
//---------------------------------------------------------------------------
{
.инфинитив
    чь                  // те-чь
.повел
    ки,,"11"            // те-ги(те)
.время Н        // настоящее время
  .лицо 1
    '',,"91"            // те-ку, те-чем
  .лицо 2
    че,,"13"            // те-че-шь, те-че-те
  .лицо 3
    '',,"92"            // те-чет, те-кут
  .действ
    кущ,,"16"           // те-кущ-ий
.время П        // прошедшее время
    к,,"36"             // те-к(-,-ла,-ло,-ли)
  .действ
    кш,,"16"            // те-кш-ий
  .деепр
    кши                 // те-кши
}

