    ки,,"11"            // вволо-ки(те)
.время Б        // будущее время
  .лицо 1
    '',,"91"            // вволо-ку, вволо-чем
  .лицо 2
    че,,"13"            // вволо-че-шь, вволо-че-те
  .лицо 3
    '',,"92"            // вволо-чет, вволо-кут
.время П        // прошедшее время
    к,,"36"             // вволо-к(-,-ла,-ло,-ли)
  .действ
    кш,,"16"            // вволо-кш-ий
  .страд
    чен,,"6"            // вволо-чен-ный
  .деепр
    кши                 // вволо-кши
}

