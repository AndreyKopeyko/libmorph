.тип нсв 16b/c''БЯ
//**************************************************************************
// Пример: житься
//**************************************************************************
{
.инфинитив
    ть,,"1"             // жи-ться
.время Н        // настоящее время
  .лицо 3
    в,,"27"             // жи-в-ется
.время П
    л,,"26"             // жи-л-ось
}

