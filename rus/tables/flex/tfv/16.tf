    ви,,"11"            // плы-ви(те)
.время Н        // настоящее время
  .лицо 1
    в,,"31"             // плы-в-у, плы-в-ем
  .лицо 2
    ве,,"13"            // плы-ве-шь, плы-ве-те
  .лицо 3
    в,,"32"             // плы-в-ет, плы-в-ут
  .действ
    вущ,,"16"           // плы-вущ-ий
  .деепр
    вя                  // плы-вя
.время П
    л,,"15"             // плы-л (-,-а,-о,-и)
  .действ
    вш,,"16"            // плы-вш-ий
  .деепр
    в                   // плы-в
    вши                 // плы-вши
}

